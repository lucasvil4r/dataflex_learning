Use dfAllEnt.pkg // Isso é suficiente para executar a maioria dos aplicativos.

Procedure LearnVariables
    String sText //Now define the variable within the PROCEDURE:
    
    Move "Hello from variable" to sText
    
    Send Info_Box sText "Hello"
End_Procedure

Send LearnVariables

////////////////////////////////////

Procedure LearnVariables
    Integer iVal1 iVal2 //Type
    
    Move 5 to iVal1
    Move (iVal1 * 7) to iVal2
    
    Send Info_Box iVal2 "Hello"
End_Procedure

Send LearnVariables

////////////////////////////////////

Procedure LearnVariables
    Integer iVal1 iVal2
    
    Move 5 to iVal1
    Move (iVal1 / 2) to iVal2
    
    Send Info_Box iVal2 "Hello"
End_Procedure

Send LearnVariables

//////////////////////////////////

Procedure LearnVariables
    Number iVal1 iVal2
    
    Move 5 to iVal1
    Move (iVal1 / 2) to iVal2
    
    Send Info_Box iVal2 "Hello"
End_Procedure

Send LearnVariables

